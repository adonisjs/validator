{
  "name": "@adonisjs/validator",
  "version": "5.0.3",
  "description": "Validator for adonis framework",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "virk",
  "license": "MIT",
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.0",
    "indicative": "^5.0.5",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@adonisjs/ace": "^5.0.0",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/sink": "^1.0.15",
    "clone": "^2.1.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^11.4.1",
    "standard": "^11.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-validation-provider.git"
  },
  "keywords": [
    "validator",
    "schema-validation",
    "adonis"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-validation-provider/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-validation-provider#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "bin",
      "test"
    ]
  }
}
